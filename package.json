{
    "name": "@devlab-ys/angular-components",
    "version": "0.0.7-development",
    "publishConfig": {
        "registry": "https://npm.pkg.github.com/"
    },
    "scripts": {
        "build": "npm run test && ng build",
        "build:quick": "npm run prebuild && ng build --configuration=development && npm run extract-tw-classes",
        "build:watch": "ng build --watch --configuration=development",
        "extract-tw-classes": "node scripts/extract-tw-classes.js",
        "lint": "ng lint",
        "merge-translations": "node scripts/merge-translations.js",
        "postbuild": "npm run extract-tw-classes",
        "prebuild": "npm run merge-translations",
        "test": "ng test --browsers ChromeHeadlessNoSandbox --watch=false",
        "test:watch": "ng test --browsers ChromeDebugging --watch --source-map",
        "update-translations": "npm run merge-translations && node scripts/update-translations.js"
    },
